<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="rectangle">
            <gradient
                android:centerX="0.5"
                android:centerY="0.5"
                android:endColor="@color/white"
                android:gradientRadius="160%"
                android:startColor="@color/Black"
                android:type="radial" />
            <corners
                android:radius="@dimen/radiusInner1x"/>
        </shape>
    </item>

    <item
        android:bottom="@dimen/tableInner1x"
        android:left="@dimen/tableInner1x"
        android:right="@dimen/tableInner1x"
        android:top="@dimen/tableInner1x">
        <shape android:shape="rectangle"
            android:thickness="@dimen/_36sdp">
            <solid
                android:color="@color/Black"/>
            <corners
                android:radius="@dimen/radiusInner2x"/>
        </shape>
    </item>

    <item
        android:bottom="@dimen/tableInner2x"
        android:left="@dimen/tableInner2x"
        android:right="@dimen/tableInner2x"
        android:top="@dimen/tableInner2x">
        <shape android:shape="rectangle">
            <solid
                android:color="@color/tableBrown"/>
            <corners
                android:radius="@dimen/radiusInner3x"/>
        </shape>
    </item>

</layer-list>