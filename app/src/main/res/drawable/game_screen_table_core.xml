<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:bottom="@dimen/tableInner1"
        android:left="@dimen/tableInner1"
        android:right="@dimen/tableInner1"
        android:top="@dimen/tableInner1">
        <shape android:shape="rectangle"
            android:thickness="@dimen/_36sdp">
            <solid
                android:color="@color/borderNblueDark3b"/>
            <corners
                android:radius="@dimen/radiusInner2"/>
        </shape>
    </item>

    <item
        android:bottom="@dimen/tableInner2"
        android:left="@dimen/tableInner2"
        android:right="@dimen/tableInner2"
        android:top="@dimen/tableInner2">
        <shape android:shape="rectangle">
            <gradient
                android:centerColor="@color/borderNblueDark1b"
                android:centerX="0.5"
                android:centerY="0.5"
                android:endColor="@color/borderNblueDark2b"
                android:gradientRadius="100%p"
                android:startColor="@color/borderNblueDark0b"
                android:type="radial" />
            <corners
                android:radius="@dimen/radiusInner3"/>
        </shape>
    </item>

</layer-list>