<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/iconWidth"
    android:layout_height="wrap_content"
    android:elevation="10dp">

    <com.facebook.shimmer.ShimmerFrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iconText"
        android:layout_marginBottom="@dimen/iconPaddingNeg"
        android:layout_alignParentTop="true"
        app:shimmer_base_alpha="0.9"
        app:shimmer_duration="1800"
        app:shimmer_highlight_alpha="1"
        app:shimmer_highlight_color="@color/white"
        app:shimmer_intensity="1"
        app:shimmer_repeat_delay="500"
        app:shimmer_shape="linear" >
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/blacksquarebutton"
            android:scaleType="fitXY"
            android:scaleX="1.08"
            android:scaleY="1.06"/>
    </com.facebook.shimmer.ShimmerFrameLayout>
    <ImageView
        android:id="@+id/iconImage1"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/iconHeight"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/iconPadding"
        android:layout_centerHorizontal="true"
        android:src="@drawable/settings"
        android:contentDescription="@null" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/iconText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/iconImage1"
        android:layout_gravity="bottom"
        android:layout_marginTop="@dimen/_4sdp"
        android:layout_marginBottom="@dimen/iconPadding"
        android:gravity="center"
        android:text="@string/createRoom"
        android:textSize="@dimen/_13ssp"
        android:textColor="@color/white"
        android:theme="@style/Theme.App" />
</RelativeLayout>


